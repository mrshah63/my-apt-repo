{
  "version": 2,
   "public": true,
  "rewrites": [
    { "source": "/(.*)", "destination": "/$1" }
  ],
  "builds": [
    {
      "src": "api/serve-file.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/pool/(.*)",
      "dest": "/api/serve-file.js?file=$1"
    },
    {
      "src": "/dists/(.*)",
      "dest": "/api/serve-file.js?file=$1"
    }
  ]
}
