{
  "version": 2,
  "builds": [
    {
      "src": "api/serve-file.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/pool/(.*)",
      "destination": "/api/serve-file.js?file=$1"
    },
    {
      "source": "/dists/(.*)",
      "destination": "/api/serve-file.js?file=$1"
    }
  ]
}
